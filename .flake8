[flake8]
ignore = 
      E501, 
      E265,
      F811,
      B010,
      PT001,
      VNE003,
      DJ01,
      PIE783,
      PIE785,
      SIM113,
      SIM102,
      I005,
      I001,
      E902,  # FileNotFoundError
max-line-length = 120
exclude =
      static,
      venv,
      migrations,
      frontend,
      .git,
      __pycache__
